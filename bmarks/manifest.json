{
    "name": "BMarks",
    "description": "Change the current page color",
    "version": "1.0",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action": {
        "default_title": "BMarks",
        "default_icon": "images/16.png",
        "default_popup": "popup.html"
    },
    "icons": { "48": "images/48.png" },
    "manifest_version": 2
}
